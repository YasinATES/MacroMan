<h2>Üyeler</h2>
<br>
{{ include('admin/messages.html.twig') }}
<a href="{{ path("admin_user_insert") }}" class="btn btn-info btn-md btn-block"> <span
            class="glyphicon glyphicon-plus"></span> Üye Ekle</a>
<br> <br>
<table class="table">
    <tbody>
    <tr>
        <th>Id</th>
        <th>Ad Soyad</th>
        <th>Email</th>
        <th>Şifre</th>
        <th>Telefon</th>
        <th>Adres</th>
        <th>Tip</th>
        <th>Durum</th>
        <th>Şehir</th>
        <th>Düzenle</th>
        <th>Sil</th>
    </tr>
    {% for x in Users %}
        <tr>
            <td>{{ x.id }}</td>
            <td>{{ x.name }}</td>
            <td>{{ x.email }}</td>
            <td>{{ x.password }}</td>
            <td>{{ x.tel }}</td>
            <td>{{ x.addres }}</td>
            <td>{{ x.type }}</td>
            <td>{{ x.status }}</td>
            <td>{{ x.city }}</td>
            <td><a href="{{ path('admin_user_update',{'id':x.id}) }}" class="btn btn-block btn-primary btn-xs"><span
                            class="glyphicon glyphicon-pencil"></span> &nbsp; &nbsp; Düzenle</a></td>
            <td><a href="{{ path('admin_user_delete',{'id':x.id}) }}"
                   onclick="return confirm('Silme İşlemi Yapılacak Emin Misiniz?')"
                   class="btn btn-block btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span>&nbsp;&nbsp;
                    Sil</a></td>
        </tr>
    {% endfor %}
    </tbody>
</table>
